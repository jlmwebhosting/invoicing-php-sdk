				</xs:annotation>
				<xs:sequence>
					<xs:element name="merchantEmail" type="xs:string" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Invoice creator's email.
							</xs:documentation>
							<xs:appinfo>
								<annox:annotate target="field">
   									<ov:NotBlank />
									<ov:NotNull />
   									<ov:MaxLength value="260"/>
   									<ppvc:PPUserNameFormatValidation />
   								</annox:annotate>
    						</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="payerEmail" type="xs:string" minOccurs="1" maxOccurs="1">
