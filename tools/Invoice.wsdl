								<annox:annotate target="field">
									<ov:MaxLength value="25" />
								</annox:annotate>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="merchantInfo" type="inv:BusinessInfoType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Company contact information of the merchant company sending the invoice.
							</xs:documentation>
							<xs:appinfo>
								<annox:annotate target="field">
									<ppv:NestedValidation />
								</annox:annotate>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="items" type="inv:InvoiceItemType" minOccurs="1" maxOccurs="100">
						<xs:annotation>
							<xs:documentation>
								List of items included in this invoice.
							</xs:documentation>
							<xs:appinfo>
								<annox:annotate target="field">
									<ov:NotNull appliesTo="CONTAINER" />
									<ov:MinSize value="1" appliesTo="CONTAINER" />
									<ppv:NestedValidation />
								</annox:annotate>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="currencyCode" type="xs:string" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Currency used for all invoice item amounts and totals.
							</xs:documentation>
							<xs:appinfo>
								<annox:annotate target="field">
									<ov:MaxLength value="3" />
									<ov:NotNull/>
									<ov:NotBlank/>
								</annox:annotate>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="invoiceDate" type="xs:dateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Date on which the invoice will be enabled.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="dueDate" type="xs:dateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Date on which the invoice payment is due.
							</xs:documentation>
							<xs:appinfo>
    							<annox:annotate target="field">
									<ov:NotNull when="groovy:_this.paymentTerms.toString().equals('DUE_ON_DATE_SPECIFIED')" />
								</annox:annotate>
							</xs:appinfo>	
						</xs:annotation>
					</xs:element>
					<xs:element name="paymentTerms" type="inv:PaymentTermsType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Terms by which the invoice payment is due.
							</xs:documentation>
							<xs:appinfo>
								<annox:annotate target="field">
									<ov:NotNull/>
									<ov:NotBlank />
									<ov:MatchPattern matchAll="true" pattern="DUE_ON_RECEIPT|DUE_ON_DATE_SPECIFIED|NET_10|NET_15|NET_30|NET_45" /> -->
									<ppv:NestedValidation />
								</annox:annotate>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="discountPercent" type="xs:decimal" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								A discount percent applied to the invoice, if any.
							</xs:documentation>
							<xs:appinfo>
								<annox:annotate target="field">
									<ov:Min value="0" />
									<ov:Max value="100" />
								</annox:annotate>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="discountAmount" type="xs:decimal" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								A discount amount applied to the invoice, if any.  If DiscountPercent is provided, DiscountAmount is ignored.
							</xs:documentation>
							<xs:appinfo>
								<annox:annotate target="field">
									<ov:NotNegative />
								</annox:annotate>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="terms" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								General terms for the invoice.
							</xs:documentation>
							<xs:appinfo>
								<annox:annotate target="field">
									<ov:MaxLength value="1000" />
								</annox:annotate>
							</xs:appinfo>	
						</xs:annotation>
					</xs:element>
					<xs:element name="note" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Note to the payer company.
							</xs:documentation>
							<xs:appinfo>
								<annox:annotate target="field">
									<ov:MaxLength value="1000" />
								</annox:annotate>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="merchantMemo" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Memo for book keeping that is private to the Merchant.
							</xs:documentation>
							<xs:appinfo>
								<annox:annotate target="field">
									<ov:MaxLength value="150" />
								</annox:annotate>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="billingInfo" type="inv:BusinessInfoType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Billing information for the payer.
							</xs:documentation>
							<xs:appinfo>
								<annox:annotate target="field">
									<ppv:NestedValidation />
								</annox:annotate>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="shippingInfo" type="inv:BusinessInfoType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Shipping information for the payer.
							</xs:documentation>
							<xs:appinfo>
								<annox:annotate target="field">
									<ppv:NestedValidation />
								</annox:annotate>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="shippingAmount" type="xs:decimal" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Cost of shipping.
							</xs:documentation>
							<xs:appinfo>
								<annox:annotate target="field">
									<ov:NotNegative />
								</annox:annotate>	
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="shippingTaxName" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Name of the applicable tax on shipping cost, if any.
							</xs:documentation>
							<xs:appinfo>
								<annox:annotate target="field">
									<ov:MaxLength value="10" />
									<ov:NotNull when="groovy:_this.shippingTaxRate != null" />
								</annox:annotate>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="shippingTaxRate" type="xs:decimal" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Rate of the applicable tax on shipping cost, if any.
							</xs:documentation>
							<xs:appinfo>
								<annox:annotate target="field">
									<ov:Min value="0" />
									<ov:Max value="100" />
									<ov:NotNull when="groovy:_this.shippingTaxName != null" />
								</annox:annotate>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:any maxOccurs="unbounded" minOccurs="0" processContents="lax" namespace="##other"/>
				</xs:sequence>
			</xs:complexType>

