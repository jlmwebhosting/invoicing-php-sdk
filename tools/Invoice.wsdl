							<xs:documentation>
								Email to which the invoice will be sent.
							</xs:documentation>
							<xs:appinfo>
								<annox:annotate target="field">
   									<ov:NotBlank />
									<ov:NotNull />
   									<ov:MaxLength value="260"/>
   									<ppvc:PPUserNameFormatValidation />
   								</annox:annotate>
    						</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="number" type="xs:string" minOccurs="0" maxOccurs="1">
